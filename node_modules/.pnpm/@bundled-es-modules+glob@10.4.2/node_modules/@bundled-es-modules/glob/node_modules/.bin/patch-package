#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/krissy.conant/Krissy/sigil-design-system/node_modules/.pnpm/patch-package@8.0.0/node_modules/patch-package/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules/.pnpm/patch-package@8.0.0/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/krissy.conant/Krissy/sigil-design-system/node_modules/.pnpm/patch-package@8.0.0/node_modules/patch-package/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules/.pnpm/patch-package@8.0.0/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../patch-package@8.0.0/node_modules/patch-package/index.js" "$@"
else
  exec node  "$basedir/../../../../../../patch-package@8.0.0/node_modules/patch-package/index.js" "$@"
fi
