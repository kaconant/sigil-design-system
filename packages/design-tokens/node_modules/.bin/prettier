#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/krissy.conant/Krissy/sigil-design-system/packages/design-tokens/node_modules/.pnpm/prettier@3.5.3/node_modules/prettier/bin/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/design-tokens/node_modules/.pnpm/prettier@3.5.3/node_modules/prettier/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/design-tokens/node_modules/.pnpm/prettier@3.5.3/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/design-tokens/node_modules/.pnpm/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/design-tokens/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules:/Users/krissy.conant/Krissy/node_modules:/Users/krissy.conant/node_modules:/Users/node_modules:/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/krissy.conant/Krissy/sigil-design-system/packages/design-tokens/node_modules/.pnpm/prettier@3.5.3/node_modules/prettier/bin/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/design-tokens/node_modules/.pnpm/prettier@3.5.3/node_modules/prettier/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/design-tokens/node_modules/.pnpm/prettier@3.5.3/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/design-tokens/node_modules/.pnpm/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/design-tokens/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules:/Users/krissy.conant/Krissy/node_modules:/Users/krissy.conant/node_modules:/Users/node_modules:/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../prettier/bin/prettier.cjs" "$@"
else
  exec node  "$basedir/../prettier/bin/prettier.cjs" "$@"
fi
