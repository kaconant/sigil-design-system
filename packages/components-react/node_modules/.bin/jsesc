#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/krissy.conant/Krissy/sigil-design-system/packages/components-react/node_modules/.pnpm/jsesc@3.1.0/node_modules/jsesc/bin/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/components-react/node_modules/.pnpm/jsesc@3.1.0/node_modules/jsesc/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/components-react/node_modules/.pnpm/jsesc@3.1.0/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/components-react/node_modules/.pnpm/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/components-react/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules:/Users/krissy.conant/Krissy/node_modules:/Users/krissy.conant/node_modules:/Users/node_modules:/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/krissy.conant/Krissy/sigil-design-system/packages/components-react/node_modules/.pnpm/jsesc@3.1.0/node_modules/jsesc/bin/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/components-react/node_modules/.pnpm/jsesc@3.1.0/node_modules/jsesc/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/components-react/node_modules/.pnpm/jsesc@3.1.0/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/components-react/node_modules/.pnpm/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/components-react/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/packages/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules:/Users/krissy.conant/Krissy/node_modules:/Users/krissy.conant/node_modules:/Users/node_modules:/node_modules:/Users/krissy.conant/Krissy/sigil-design-system/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jsesc/bin/jsesc" "$@"
else
  exec node  "$basedir/../jsesc/bin/jsesc" "$@"
fi
